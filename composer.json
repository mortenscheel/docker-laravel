{
    "name": "mortenscheel/laravel-docker",
    "description": "Docker helper for Laravel projects",
    "keywords": ["laravel", "docker", "console", "cli"],
    "homepage": "https://github.com/mortenscheel/laravel-docker",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/mortenscheel/laravel-docker/issues",
        "source": "https://github.com/mortenscheel/laravel-docker"
    },
    "authors": [
        {
            "name": "Morten Scheel",
            "email": "morten@mortenscheel.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel-zero/framework": "^9.2",
        "nunomaduro/termwind": "^1.14",
        "sebastian/diff": "^4.0"
    },
    "require-dev": {
        "intonate/tinker-zero": "^1.2",
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.3",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["docker-laravel"]
}
