{
    "name": "mortenscheel/docker-laravel",
    "description": "Docker helper for Laravel projects",
    "keywords": ["laravel", "docker", "console", "cli"],
    "homepage": "https://github.com/mortenscheel/laravel-docker",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/mortenscheel/laravel-docker/issues",
        "source": "https://github.com/mortenscheel/laravel-docker"
    },
    "authors": [
        {
            "name": "Morten Scheel",
            "email": "morten@mortenscheel.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel-zero/phar-updater": "^1.3"
    },
    "require-dev": {
        "laravel-zero/framework": "^10.0",
        "nunomaduro/termwind": "^1.15.1",
        "laravel/pint": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/docker-laravel"]
}
