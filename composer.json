{
    "name": "mortenscheel/docker-laravel",
    "description": "Docker helper for Laravel projects",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "laravel",
        "docker",
        "console",
        "cli"
    ],
    "authors": [
        {
            "name": "Morten Scheel",
            "email": "morten@mortenscheel.com"
        }
    ],
    "homepage": "https://github.com/mortenscheel/laravel-docker",
    "support": {
        "issues": "https://github.com/mortenscheel/laravel-docker/issues",
        "source": "https://github.com/mortenscheel/laravel-docker"
    },
    "require": {
        "php": "^8.1",
        "laravel-zero/phar-updater": "^1.3",
        "sebastian/diff": "^5.0.3"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.31",
        "laravel-zero/framework": "^10.1",
        "laravel/pint": "^1.10.3",
        "nunomaduro/larastan": "^2.6.3",
        "nunomaduro/termwind": "^1.15.1",
        "phpro/grumphp": "^1.16",
        "yieldstudio/grumphp-laravel-pint": "^0.0.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "bin": [
        "builds/docker-laravel"
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpro/grumphp": true,
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
